<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="468.0" prefWidth="756.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ReadMidTermAndGenerateReportController">
   <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="75.0" prefHeight="300.0" prefWidth="791.0">
         <children>
            <HBox alignment="CENTER" prefHeight="86.0" prefWidth="791.0" spacing="20.0">
               <children>
                  <Button id="studentCourseFilePathButton" fx:id="studentCourseFilePathButton" alignment="CENTER" mnemonicParsing="false" onAction="#onStudentCourseFilePathButtonClick" prefHeight="55.0" prefWidth="230.0" text="Student Course File Path">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Label fx:id="studentCourseFilePathLabel" prefHeight="21.0" prefWidth="318.0" text="(Student Course File Path not selected)" wrapText="true" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="86.0" prefWidth="791.0" spacing="20.0">
               <children>
                  <Button fx:id="midTermDateSheetFilePathButton" alignment="CENTER" mnemonicParsing="false" onAction="#onMidTermDateSheetFilePathButtonClick" prefHeight="55.0" prefWidth="230.0" text="Mid Term Date Sheet File Path" />
                  <Label fx:id="midTermDateSheetFilePathLabel" prefHeight="21.0" prefWidth="316.0" text="(Mid Term Date Sheet File Path not selected)" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="86.0" prefWidth="791.0" spacing="20.0">
               <children>
                  <Button fx:id="outputDirectoryPathButton" alignment="CENTER" mnemonicParsing="false" onAction="#onOutputDirectoryPathButtonClick" prefHeight="55.0" prefWidth="230.0" text="Output Directory Path" />
                  <Label fx:id="outputDirectoryPathLabel" prefHeight="21.0" prefWidth="316.0" text="(Output Directory Path not selected)" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="86.0" prefWidth="791.0">
               <children>
                  <Button fx:id="generateButton" alignment="CENTER" mnemonicParsing="false" onAction="#onGenerateMidTermReportButtonClick" prefHeight="55.0" prefWidth="205.0" text="Generate Mid Term Report">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Label prefHeight="21.0" prefWidth="236.0" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
