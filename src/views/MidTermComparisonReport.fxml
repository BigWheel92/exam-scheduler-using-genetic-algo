<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="525.0" prefWidth="952.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MidTermComparisonReportController">
   <children>
      <VBox layoutX="147.0" layoutY="98.0" prefHeight="300.0" prefWidth="659.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="91.0" prefWidth="799.0" spacing="20.0">
               <children>
                  <Button fx:id="oldMidTermReportButton" alignment="CENTER" mnemonicParsing="false" onAction="#onOldMidTermReportButtonClick" prefHeight="62.0" prefWidth="226.0" text="Old Mid Report Path">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Label fx:id="oldMidTermReportLabel" prefHeight="21.0" prefWidth="276.0" text="(No Old Mid Report Path Selected)" wrapText="true" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="86.0" prefWidth="791.0" spacing="20.0">
               <children>
                  <Button fx:id="newMidTermReportButton" alignment="CENTER" mnemonicParsing="false" onAction="#onNewMidTermReportButtonClick" prefHeight="55.0" prefWidth="230.0" text="New Mid Report Path" />
                  <Label fx:id="newMidTermReportLabel" prefHeight="21.0" prefWidth="268.0" text="(No New Mid Report Path Selected)" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button fx:id="outputPathButton" mnemonicParsing="false" onAction="#onOutputPathButtonClick" prefHeight="53.0" prefWidth="231.0" text="output path" />
                  <Label fx:id="outputLabel" text="(No Output Path Selected)" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="86.0" prefWidth="791.0" spacing="20.0">
               <children>
                  <Button fx:id="generateButton" alignment="CENTER" mnemonicParsing="false" onAction="#onGenerateButtonClick" prefHeight="55.0" prefWidth="220.0" text="Generate Comparison Report">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
